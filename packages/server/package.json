{
  "name": "@desafio-martinello/server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn  --inspect --transpile-only --exit-child --watch src src/server.ts",
    "dev2": "ts-node-dev --poll --respawn  ./src/server.ts",
    "dev3": "nodemon --legacy-watch --watch 'src/*.ts' --exec 'ts-node' src/server.ts",
    "typeorm": "ts-node-dev /../../node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@prisma/client": "^3.6.0",
    "express": "^4.17.1",
    "express-validator": "^6.14.0",
    "ts-node": "^10.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/uuid": "^8.3.3",
    "prisma": "^3.6.0",
    "ts-node-dev": "^1.1.8"
  }
}
